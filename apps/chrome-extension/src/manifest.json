{
  "manifest_version": 3,
  "name": "RAG Bookmarks",
  "version": "1.0.0",
  "description": "Save URLs to your RAG Bookmarks system",
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
